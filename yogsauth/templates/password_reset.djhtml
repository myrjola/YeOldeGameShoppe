{% extends "base.djhtml" %}
{% load staticfiles %}
{% load bootstrap3 %}

{% block content %}

<form action={% url 'password_reset' %} method="post" class="form">
    {% csrf_token %}
    {% bootstrap_form form %}
    {% buttons %}
    <button type="submit" class="btn btn-primary">Submit</button>
    {% endbuttons %}
</form>
{% endblock %}
